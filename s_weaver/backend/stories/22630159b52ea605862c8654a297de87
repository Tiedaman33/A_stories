
print function for %
#include "main.h"
/**
 * print_37 - prints a %
 *
 * Return: 0 success
 */
int print_37(void)
{
        _putchar(37);
        return (0);
}

print function for string
#include "main.h"
/**
 * printf_char - prints a char.
 * @val: args
 * Return: 1
 */
int printf_char(va_list val)
{
        char str;

        str = va_arg(val, int);
        _putchar(str);
        return (1);
}

print function for stringl
#include "main.h"
/**
 * print_string - print a string.
 * @val: argumnets
 * Return: the lenght of string.
 */
int print_string(va_list val)
{
        char *str;
        int i;
        int length;

        str = va_arg(val, char *);
        if (str == NULL)
        {
                str = "(null)";
                length = _strlen(str);
                for (i = 0; i < length; i++)
                        _putchar(str[i]);
                return (length);
        }
        else
        {
                length = _strlen(str);
                for (i = 0; i < length; i++)
                        _putchar(str[i]);
                return (length);
        }
}

print function for print stringlen
#include "main.h"
/**
 * _strlen - Rturns the legth of a string
 * @str: string pointer
 * Return: 1
 */
int _strlen(char *str)
{
        int i;

        for (i = 0; str[i] != 0; i++)
                ;
        return (i);
}
/**
 * _strlenc - strlen fucntion
 * @str: char pointer
 * Return: i
 */
int _strlenc(const char *str)
{
        int i;

        for (i = 0; str[i] != 0; i++)
                ;
        return (i);
}
